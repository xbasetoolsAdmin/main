var v_aa =0;
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
        if (v_aa == 1) {
          $("#menu-toggle").html('<span class="fa fa align-icon-right"></span>').show();
          v_aa =0;
        }
        else {
          $("#menu-toggle").html('<span class="fa fa align-icon-right"></span>').show();
          v_aa =1;     
        }
        
    });

    </script>
