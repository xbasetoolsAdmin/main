   function deletrdps(id){
	   var type = $("#shop"+id).attr('type')
	$("#shop"+id).modal('processing ..').show();
	$.ajax({
	METHOD: 		'GET',
     url:"./buytool.php?id="+id+"&t=rdps",
	success:	function(data)
	{
		$("#shop"+id).html(data).show();
	}});
}
